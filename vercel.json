{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs18.x"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "src/index.js"
    },
    {
      "source": "/bookmarks/:path*",
      "destination": "src/index.js"
    },
    {
      "source": "/folders/:path*",
      "destination": "src/index.js"
    },
    {
      "source": "/chat/:path*",
      "destination": "src/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "src/index.js"
    }
  ]
}
